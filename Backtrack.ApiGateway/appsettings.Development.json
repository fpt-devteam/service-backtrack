{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Yarp": "Debug"
    }
  },
  "Firebase": {
    "ProjectId": "backtrack-sep490",
    "ServiceAccountPath": "C:\\Users\\NHATTHANG\\FptUniversity\\SEP490\\backtrack-sep490-firebase-adminsdk-fbsvc-d625f3f3d9.json"
  },
  "ReverseProxy": {
    "Routes": {
      "core-route": {
        "ClusterId": "core-cluster",
        "Match": {
          "Path": "/api/core/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "chat-route": {
        "ClusterId": "chat-cluster",
        "Match": {
          "Path": "/api/chat/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "notifications-route": {
        "ClusterId": "notifications-cluster",
        "Match": {
          "Path": "/api/notify/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "public-route": {
        "ClusterId": "core-cluster",
        "Match": {
          "Path": "/public/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "public/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "core-cluster": {
        "Destinations": {
          "core-destination": {
            "Address": "http://localhost:8080"
          }
        },
        "HttpRequest": {
          "Timeout": "00:00:30"
        }
      },
      "chat-cluster": {
        "Destinations": {
          "chat-destination": {
            "Address": "http://localhost:3000"
          }
        },
        "HttpRequest": {
          "Timeout": "00:00:30"
        }
      },
      "notifications-cluster": {
        "Destinations": {
          "notifications-destination": {
            "Address": "http://localhost:7000"
          }
        },
        "HttpRequest": {
          "Timeout": "00:00:30"
        }
      }
    }
  }
}