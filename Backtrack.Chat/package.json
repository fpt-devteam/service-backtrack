{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "cross-env NODE_ENV=development ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "type-check": "tsc --noEmit",
    "seed:all": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./src/scripts/seed-all.ts",
    "seed": "npm run seed:all"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jet-validators": "^1.6.5",
    "module-alias": "^2.2.3",
    "mongoose": "^8.20.4",
    "morgan": "^1.10.1",
    "socket.io": "^4.8.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@swc/core": "^1.15.3",
    "@types/amqplib": "^0.10.8",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.2",
    "@types/socket.io": "^3.0.2",
    "@types/winston": "^2.4.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-n": "^17.23.1",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
